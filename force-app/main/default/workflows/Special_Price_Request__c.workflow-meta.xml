    <rules>
        <fullName>SPR%3A Approve MSP as CM</fullName>
        <actions>
            <name>SPR_Approved_By_Team_Manager</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>SPR_Date_Approved_By_Team_Manager</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>SPR_Status_Approved</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Automatically approve MSP in case the current user is the final approver.</description>
        <formula>ISPICKVAL(Price_Level__c, &quot;MSP (Minimum Sales Price)&quot;) &amp;&amp; (
(CreatedBy.Alias=&quot;tnak&quot; &amp;&amp; $User.Alias=&quot;mchi&quot;) ||
(CreatedBy.Alias=&quot;ntha&quot; &amp;&amp; $User.Alias=&quot;ntha&quot;) ||
(CreatedBy.Alias=&quot;psbi&quot; &amp;&amp; $User.Alias=&quot;psbi&quot;) ||
(CreatedBy.Alias=&quot;sguo&quot; &amp;&amp; $User.Alias=&quot;sguo&quot;) ||
(CreatedBy.Alias=&quot;shki&quot; &amp;&amp; $User.Alias=&quot;shki&quot;) ||
(CreatedBy.Alias=&quot;tnak&quot; &amp;&amp; $User.Alias=&quot;tnak&quot;) ||
(CreatedBy.Alias=&quot;tzho&quot; &amp;&amp; $User.Alias=&quot;tzho&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
