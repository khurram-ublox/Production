        <active>false</active>
        <description>Clear Status and Approved By Regional Head checkbox if SPR (MSP) is created by Regional Head.</description>
        <formula>ISCHANGED(Price_Level__c) &amp;&amp;
NOT(ISPICKVAL(Price_Level__c,&quot;MSP (Minimum Sales Price)&quot;)) &amp;&amp; 
/*ISPICKVAL(Price_Level__c, &quot;PPP (Project Price Plan)&quot;) &amp;&amp;*/
ISPICKVAL(Status__c, &quot;Approved&quot;) &amp;&amp;
NOT(Approved_By_Team_Leader__c) &amp;&amp;
NOT(Approved_By_Team_Manager__c) &amp;&amp;
Approved_By_Regional_Head__c &amp;&amp; 
(
(CreatedBy.Alias=&quot;atan&quot; &amp;&amp; $User.Alias=&quot;atan&quot;) ||
(CreatedBy.Alias=&quot;egui&quot; &amp;&amp; $User.Alias=&quot;egui&quot;) ||
(CreatedBy.Alias=&quot;sram&quot; &amp;&amp; $User.Alias=&quot;sram&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
