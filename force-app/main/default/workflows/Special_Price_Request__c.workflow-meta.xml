    <rules>
        <fullName>SPR%3A Add Email PS All</fullName>
        <actions>
            <name>SPR_Add_Email_PS_CEL</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>SPR_Add_Email_PS_POS</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>SPR_Add_Email_PS_SHO</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>(CreatedDate&lt;&gt;LastModifiedDate) &amp;&amp; Total_Quantity__c=0 &amp;&amp; LEFT(TEXT(Price_Level__c),3)=&quot;PPP&quot;</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>SPR%3A Add Email PS CEL</fullName>
        <actions>
            <name>SPR_Add_Email_PS_CEL</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Special_Price_Request__c.Total_Quantity_CEL__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>Special_Price_Request__c.Price_Level__c</field>
            <operation>equals</operation>
            <value>PPP (Project Price Plan)</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
