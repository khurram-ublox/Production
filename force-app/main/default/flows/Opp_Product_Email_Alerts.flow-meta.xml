            </conditions>
            <conditions>
                <leftValueReference>OppProd_Record.LastModifiedDate</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>EMMY_Date_Formula</elementReference>
                </rightValue>
            </conditions>
