    <decisions>
        <name>Opp_Product_Centre_is_Cellular</name>
        <label>Opp Product Centre is Cellular ?</label>
        <locationX>491</locationX>
        <locationY>470</locationY>
        <defaultConnector>
            <targetReference>Opp_Product_Series_is_M8_9_10</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Cellular_Product</name>
            <conditionLogic>1 AND 2 AND ( 3 OR  4 OR 5) AND 6</conditionLogic>
            <conditions>
                <leftValueReference>OppProd_Record.Product__r.Product_Center__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cellular</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>RecordIsNew</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>OppProd_Record.Product__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>SARA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>OppProd_Record.Product__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>LARA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>OppProd_Record.Product__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>LENA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>OppProd_Record.Opportunity__r.Owner.IsActive</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Opportunity_Task</targetReference>
            </connector>
            <label>Cellular Product</label>
        </rules>
    </decisions>
