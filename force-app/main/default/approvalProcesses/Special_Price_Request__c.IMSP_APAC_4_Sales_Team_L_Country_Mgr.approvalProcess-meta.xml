            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <label>(MSP 4) Approval by Team Leader</label>
        <name>MSP_4_Approval_by_Team_Leader</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>SPR_Approved_By_Team_Manager</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Date_Approved_By_Team_Manager</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Fill_Prices_Valid_Until</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Fill_Team_Manager_Email</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Request_approved</name>
                <type>Alert</type>
            </action>
            <action>
                <name>SPR_Status_Approved</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Special_Price_Request__c.Approved_By_Team_Leader__c</field>
                <operation>equals</operation>
                <value>True</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>(MSP 4) Approval by Team Manager</label>
        <name>MSP_4_Approval_by_Team_Manager</name>
        <rejectBehavior>
            <type>BackToPrevious</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>SPR_Date_Rejected</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Request_rejected</name>
                <type>Alert</type>
            </action>
            <action>
                <name>SPR_Status_Rejected</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>SPR_Approved_By_Regional_Head_EMEA</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Date_Approved_By_Regional_Head</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Fill_Prices_Valid_Until</name>
