            </action>
            <action>
                <name>SPR_Request_approved</name>
                <type>Alert</type>
            </action>
            <action>
                <name>SPR_Status_Approved</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Special_Price_Request__c.Approved_By_Team_Manager__c</field>
                <operation>equals</operation>
                <value>True</value>
            </criteriaItems>
        </entryCriteria>
        <label>(MSP 4) Approval by Regional Head</label>
        <name>MSP_4_Approval_by_Regional_Head</name>
        <rejectBehavior>
            <type>BackToPrevious</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>SPR_Date_Rejected</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>SPR_Request_rejected</name>
                <type>Alert</type>
            </action>
            <action>
                <name>SPR_Status_Rejected</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <description>MSP APAC Process: Sales Manager &gt; Sales Team Leader &gt; Country Manager
Request initiator: APAC Sales reporting to Sales Team Leader in CN, JP (kake, myos, dwan, mhua, mili, nage, tzha)</description>
    <emailTemplate>unfiled$public/SPR_Request_update_notification</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>ISPICKVAL(Price_Level__c, &quot;MSP (Minimum Sales Price)&quot;) &amp;&amp;
NOT(OR(ISPICKVAL($User.uB_Office__c, &quot;EMEA&quot;),ISPICKVAL($User.uB_Office__c, &quot;Americas&quot;),ISPICKVAL($User.uB_Office__c, &quot;Corporate&quot;))) &amp;&amp;
($User.Alias=&quot;kake&quot; ||
$User.Alias=&quot;myos&quot; ||
$User.Alias=&quot;dwan&quot; ||
$User.Alias=&quot;mhua&quot; ||
$User.Alias=&quot;mili&quot; ||
$User.Alias=&quot;nage&quot; ||
$User.Alias=&quot;tzha&quot;)</formula>
    </entryCriteria>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
