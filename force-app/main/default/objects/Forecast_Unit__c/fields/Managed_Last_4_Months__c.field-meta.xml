<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Managed_Last_4_Months__c</fullName>
    <externalId>false</externalId>
    <formula>if( or(isnull(Last_Manual_Update__c),  Last_Manual_Update__c &lt; (today () - 120)), 0, 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>1 if last manual update withing last 120 days, 0 else</inlineHelpText>
    <label>Managed Last 4 Months</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
