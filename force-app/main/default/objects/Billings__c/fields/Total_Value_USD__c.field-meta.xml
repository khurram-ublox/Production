<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Value_USD__c</fullName>
    <externalId>false</externalId>
    <formula>if ( USDtoCHF__c = 0, Total_Value_CHF__c /  $User.USDtoCHF__c ,  Total_Value_CHF__c / USDtoCHF__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Value of order in USD, calculated with value in field USDtoCHF (ex rate of invoice date), if this field is empty then ex rate of the month is used</inlineHelpText>
    <label>Total Value USD</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
