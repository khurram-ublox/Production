<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RDR__c</fullName>
    <description>Reqest Delivery Performance</description>
    <externalId>false</externalId>
    <formula>if(  ISNULL(Request_Date__c), 1,
if (Delivery_Date__c  &lt;=  Request_Date__c , 1,0)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Reqest Delivery Performance. 1 if delivery data &lt;= request date, else 0</inlineHelpText>
    <label>RDR</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
