<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Turn_Business__c</fullName>
    <description>Checks whether order and delivery are happening in the same month</description>
    <externalId>false</externalId>
    <formula>AND(MONTH(Order_Date__c) = MONTH(Delivery_Date__c), YEAR(Order_Date__c) = YEAR(Delivery_Date__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Only used for reports: deliver the turn business ratio</inlineHelpText>
    <label>Turn Business</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
