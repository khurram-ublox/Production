<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Country_and_thingstream_should_not_blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(Country), ISBLANK(Thingstream_Domain__c))</errorConditionFormula>
    <errorDisplayField>Country</errorDisplayField>
    <errorMessage>Please select country of the lead.</errorMessage>
</ValidationRule>
